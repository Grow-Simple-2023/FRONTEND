{
  "expo": {
    "name": "mobileApp",
    "slug": "mobileApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.samyc2002.mobileApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#040C18"
      },
      "package": "com.samyc2002.mobileApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
